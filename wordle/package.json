    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.18.0",
    "angular-eslint": "18.1.0",
    "eslint": "^9.7.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.1.1",
    "prettier": "^3.3.3",
